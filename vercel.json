{
	"builds":[
		{
			"src": "src/main.ts",
			"use": "@vercel/node",
      "config": {
        "includeFiles": [
          "tmp/**",
          "model.nlp"
        ]
      }
		}
	],
	"routes":[{
		"src":"/(.*)",
		"dest":"src/main.ts",
		"methods":[
			"GET",
			"POST",
			"PUT",
			"PATCH",
			"DELETE",
			"OPTIONS"
		]
	}],
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
  }
}