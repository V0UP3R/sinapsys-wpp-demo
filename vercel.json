{
	"builds":[
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
  "api/handler.js": {
    "runtime": "nodejs22.x"
  },
	"routes":[{
		"src":"/(.*)",
		"dest":"src/main.ts",
		"methods":[
			"GET",
			"POST",
			"PUT",
			"PATCH",
			"DELETE",
			"OPTIONS"
		]
	}],
  "rewrites": [
    { "source": "/(.*)", "destination": "/src/handler.ts" }
  ]
}