{
  "builds": [
    {
			"src": "src/main.ts",
			"use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**",
          "tmp/**",
          "model.nlp"
        ],
				"exclude": [
          "**/node_modules/@sparticuz/chromium/aws/**",
          "**/node_modules/@sparticuz/chromium/bin/**",
          "**/node_modules/puppeteer/**",
          "**/node_modules/whatsapp-web.js/src/**"
        ]
      }
    }
  ],
  "routes": [
    {
			"src":"/(.*)",
			"dest":"src/main.ts",
			"methods":[
				"GET",
				"POST",
				"PUT",
				"PATCH",
				"DELETE",
				"OPTIONS"
			]
    }
  ]
}